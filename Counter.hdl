CHIP Counter {
    IN inc, reset;
    OUT out[16];

    PARTS:
    // Replace Buf with a direct connection
    Not(in=reset, out=reset_n);
    
    Mux16(a=a2, b=0, sel=reset, out=a3); 
    Mux16(a=a4, b=a5, sel=inc, out=a2);
    Inc16(in=a4, out=a5);
    Register(in=a3, load=true, out=a4, out=out);
}
