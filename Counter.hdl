CHIP Counter {
    IN inc, reset;
    OUT out[16];

    PARTS:
    Mux16(a=nextValue, b[16]=false, sel=reset, out=resetMuxOut);
    Mux16(a=currentValue, b=incrementedValue, sel=inc, out=nextValue);
    Inc16(in=currentValue, out=incrementedValue);
    Register(in=resetMuxOut, load=true, out=currentValue);
    
    // Output the current value of the counter
    OUT out = currentValue;
}
