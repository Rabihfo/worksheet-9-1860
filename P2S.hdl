CHIP Counter {
    IN inc, reset;
    OUT out[16];

    PARTS:
    // Ensure b=false is a valid 16-bit bus (should be false[16])
    Mux16(a=a2, b=false[16], sel=reset, out=a3); 

    // Ensure proper connection of buses
    Mux16(a=a4, b=a5, sel=inc, out=a2);

    // Increment a 16-bit value
    Inc16(in=a4, out=a5);

    // Store result in Register
    Register(in=a3, load=true, out=a4, out=out);
}
